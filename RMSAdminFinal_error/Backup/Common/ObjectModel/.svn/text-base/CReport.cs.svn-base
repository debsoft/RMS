using System;
using System.Collections.Generic;
using System.Text;

namespace RMS.Common.ObjectModel
{
    public class CReport
    {
        private Int64 m_oDate;
        private float m_fResturentPayment;
        private float m_fBarServicePayment;
        private float m_fWhiteRoomPayment;
        private float m_fExecutiveBarPayment;
        private float m_fTotalPayment;
        private float m_fVATPayment;

        public CReport()
        {
            m_oDate = 0;
            m_fResturentPayment = 0;
            m_fBarServicePayment = 0;
            m_fWhiteRoomPayment = 0;
            m_fExecutiveBarPayment = 0;
            m_fTotalPayment = 0;
            m_fVATPayment = 0;
        }

        public Int64 Date
        {
            get { return m_oDate; }
            set { m_oDate = value; }
        }

        public float ResturentPayment
        {
            get { return m_fResturentPayment; }
            set { m_fResturentPayment = value; }
        }


        public float BarServicePayment
        {
            get { return m_fBarServicePayment; }
            set { m_fBarServicePayment = value; }
        }

        public float WhiteRoomPayment
        {
            get { return m_fWhiteRoomPayment; }
            set { m_fWhiteRoomPayment = value; }
        }

        public float ExecutiveBarPayment
        {
            get { return m_fExecutiveBarPayment; }
            set { m_fExecutiveBarPayment = value; }
        }

        public float TotalPayment
        {
            get { return m_fTotalPayment; }
            set { m_fTotalPayment = value; }
        }


        public float VATPayment
        {
            get { return m_fVATPayment; }
            set { m_fVATPayment = value; }
        }

    }
}
