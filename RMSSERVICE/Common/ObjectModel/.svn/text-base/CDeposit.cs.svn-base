using System;
using System.Collections.Generic;
using System.Text;

namespace RMS.Common.ObjectModel
{
    public class CDeposit
    {
        private Int64 m_bDepositeID;
        private Int64 m_bOrderID;
        private double m_fCashAmount;
        private double m_fEFTAmount;
        private double m_fChequeAmount;
        private double m_fVoucherAmount;
        private double m_fAccountPayAmount;
        

        public CDeposit()
        {
            m_bDepositeID = 0;
            m_bOrderID = 0;
            m_fCashAmount = 0;
            m_fEFTAmount = 0;
            m_fChequeAmount = 0;
            m_fVoucherAmount = 0;
            m_fAccountPayAmount = 0;
        }

        public Int64 DepositeID
        {
            get { return m_bDepositeID; }
            set { m_bDepositeID = value; }
        }

        public Int64 DepositeOrderID
        {
            get { return m_bOrderID; }
            set { m_bOrderID = value; }
        }

        public double DepositeCashAmount
        {
            get { return m_fCashAmount; }
            set { m_fCashAmount = value; }
        }

        public double DepositeEFTAmount
        {
            get { return m_fEFTAmount; }
            set { m_fEFTAmount = value; }
        }

        public double DepositeChequeAmount
        {
            get { return m_fChequeAmount; }
            set { m_fChequeAmount = value; }
        }

        public double DepositeVoucherAmount
        {
            get { return m_fVoucherAmount; }
            set { m_fVoucherAmount = value; }
        }

        public double DepositeAccountPayAmount
        {
            get { return m_fAccountPayAmount; }
            set { m_fAccountPayAmount = value; }
        }
        
    }
}
