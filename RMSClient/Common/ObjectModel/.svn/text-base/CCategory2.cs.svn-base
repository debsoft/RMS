using System;
using System.Collections.Generic;
using System.Text;

namespace RMS.Common.ObjectModel
{
    public class CCategory2
    {
        private int m_iCategory2ID;
        private String m_sCategory2Name;
        private int m_iCategory1ID;
        private int m_iCategory2Order;
        private int m_iCategory2Type;
        private String m_sCategory2Color;
        private int m_iViewTable;
        private int m_iViewBar;
        private int m_iViewTakeAway;

        public CCategory2()
        {
            m_iCategory2ID = 0;
            m_sCategory2Name = String.Empty;
            m_iCategory1ID = 0;
            m_iCategory2Order = 0;
            m_iCategory2Type = 0;
            m_sCategory2Color = String.Empty;
            m_iViewTable = 0;
            m_iViewBar = 0;
            m_iViewTakeAway = 0;
        }

        public int Category2ID
        {
            get { return m_iCategory2ID; }
            set { m_iCategory2ID = value; }
        }

        public String Category2Name
        {
            get { return m_sCategory2Name; }
            set { m_sCategory2Name = value; }
        }

        public int Category1ID
        {
            get { return m_iCategory1ID; }
            set { m_iCategory1ID = value; }
        }

        public int Category2Order
        {
            get { return m_iCategory2Order; }
            set { m_iCategory2Order = value; }
        }
        
        public int Category2Type
        {
            get { return m_iCategory2Type; }
            set { m_iCategory2Type = value; }
        }

        public String Category2Color
        {
            get { return m_sCategory2Color; }
            set { m_sCategory2Color = value; }
        }

        public int Category2ViewTable
        {
            get { return m_iViewTable; }
            set { m_iViewTable = value; }
        }

        public int Category2ViewBar
        {
            get { return m_iViewBar; }
            set { m_iViewBar = value; }
        }

        public int Category2ViewTakeAway
        {
            get { return m_iViewTakeAway; }
            set { m_iViewTakeAway = value; }
        }

    }
}
