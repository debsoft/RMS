using System;
using System.Collections.Generic;
using System.Text;

namespace RMS.Common.ObjectModel
{
    public class CParentCategory
    {
        private int m_iParentCategoryID;
        private String m_sParentCategoryName;

        public CParentCategory()
		{
           m_iParentCategoryID=0;
            m_sParentCategoryName=String.Empty;
		}

        public int ParentCategoryID
		{
            get { return m_iParentCategoryID; }
            set { m_iParentCategoryID = value; }
		}

        public String ParentCategoryName
		{
            get { return m_sParentCategoryName; }
            set { m_sParentCategoryName = value; }
		}

    }
}
