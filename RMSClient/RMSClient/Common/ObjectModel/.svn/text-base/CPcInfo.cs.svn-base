using System;
using System.Collections.Generic;
using System.Text;

namespace RMS.Common.ObjectModel
{
    public class CPcInfo
    {

        private int m_iPcID;
        private String m_sPcIP;
        private String m_sName;


        public CPcInfo()
        {
            m_iPcID = 0;
            m_sPcIP = String.Empty;
            m_sName = String.Empty;

        }


        public int PcID
        {
            get { return m_iPcID; }
            set { m_iPcID = value; }
        }


        public String PcIP
        {
            get { return m_sPcIP; }
            set { m_sPcIP = value; }
        }


        public String Name
        {
            get { return m_sName; }
            set { m_sName = value; }
        }



    }
}
