using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Windows.Forms;
using System.Drawing;

namespace RMSUI
{
    public class PopCloseButton: RMSGenericButton
    {

        public PopCloseButton()
        {
            base.BackgroundImage = RMSUI.Properties.Resources.pop_close_up;
            base.BackgroundImageLayout = ImageLayout.Center;
            base.Width = 288;
            base.Height = 58;
            base.BgImageOnMouseDown = RMSUI.Properties.Resources.pop_close_down;
            base.BgImageOnMouseUp = RMSUI.Properties.Resources.pop_close_up;
            base.ForeColorOnMouseDown = Color.White;

        }

    }
}
